model Subject {
  id         String   @id @default(uuid())
  name       String
  code       String   @unique
  credits    Int
  semesterId String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  semester   Semester @relation("SemesterSubjects", fields: [semesterId], references: [id])
  Grade      Grade?
}
